    "19": {
      "id": 19,
      "type": "formalizationChecker",
      "data": {
        "exercise": {
          "exercise_id": 17,
          "title": "2023 – Cvičenie 8.1",
          "description": "Sformalizujte nasledujúce tvrdenia ako ucelenú teóriu v danom jazyku logiky prvého rádu.",
          "constants": "Evka, Novy, Theory_of_everything, Introduction_to_abstraction, Adam, Mlada, Course_for_demanding",
          "predicates": "phd_student/1, supervisor/2, human/1, student/1, teacher/1, teaches/2, full_prof/1, teaching_assistant/2, assistant_prof/1, associate_prof/1, aced/2, passed/2, enrolled_in/2, course/1, popular/1, smart/1, happy/1, likes/2",
          "functions": "",
          "constraints": "",
          "propositions": [
            {
              "proposition_id": 778,
              "proposition": "1. Evka je doktorandka a má školiteľa.",
              "solution": ""
            },
            {
              "proposition_id": 779,
              "proposition": "2. Všetko sú ľudia.",
              "solution": null
            },
            {
              "proposition_id": 780,
              "proposition": "3. Všetci doktorandi sú študenti.",
              "solution": null
            },
            {
              "proposition_id": 781,
              "proposition": "4. Niektorí doktorandi sú aj učiteľmi.",
              "solution": null
            },
            {
              "proposition_id": 782,
              "proposition": "5. Nikto nemôže učiť sám seba.",
              "solution": null
            },
            {
              "proposition_id": 783,
              "proposition": "6. Žiaden profesor nemôže byť doktorand.",
              "solution": null
            },
            {
              "proposition_id": 784,
              "proposition": "7. Niektorí študenti samozrejme nie sú doktorandi.",
              "solution": null
            },
            {
              "proposition_id": 785,
              "proposition": "8. Doktorandi profesora Nového sú cvičiaci buď na Teórii všetkého alebo Úvode do abstrakcie.",
              "solution": null
            },
            {
              "proposition_id": 786,
              "proposition": "9. Učitelia okrem asistentov sú profesori a docenti.",
              "solution": null
            },
            {
              "proposition_id": 787,
              "proposition": "10. Adamov školiteľ musí byť docent alebo profesor v prípade, že je Adam doktorand.",
              "solution": null
            },
            {
              "proposition_id": 788,
              "proposition": "11.  Docentka Mladá školí iba takých študentov, ktorí absolvovali s vyznamenaním Kurz pre náročných, ale nie sú doktorandi.",
              "solution": null
            },
            {
              "proposition_id": 789,
              "proposition": "12. Predmet Teória všetkého si nemôžu zapísať tí, čo neabsolvovali ani Kurz pre náročných, ani Úvod do abstrakcie.",
              "solution": null
            },
            {
              "proposition_id": 790,
              "proposition": "13. Ak nejaký kurz učí profesor Nový, tak ide o neobľúbený kurz.",
              "solution": null
            },
            {
              "proposition_id": 791,
              "proposition": "14. Doktorandi profesora Nového cvičiaci Teóriu všetkého sú veľmi bystrí.",
              "solution": null
            },
            {
              "proposition_id": 792,
              "proposition": "15. Ak je nejaký Mladej doktorand bystrý, tak je šťastná.",
              "solution": null
            },
            {
              "proposition_id": 793,
              "proposition": "16. Nový je šťastný, iba ak ho obľubuje aspoň jeden študent.",
              "solution": null
            }
          ]
        },
        "exerciseId": 17
      },
